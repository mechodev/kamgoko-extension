{
  "manifest_version": 3,
  "name": "Whatsapp Web Module Extension",
  "description": "Permet d'avoir pour whatsapp web un mini module de statistique",
  "version": "1.0.0",
  "content_scripts":[{
    "matches":  ["<all_urls>"],
    "js": ["content.js"],
    "css": ["style.css"]
  }],
  "permissions": ["storage", "activeTab", "scripting"],
  "action": {
    "default_popup": "extension.html"
  }
}
